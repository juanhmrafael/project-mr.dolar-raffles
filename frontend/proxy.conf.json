{
    "/api/": {
        "target": "http://backend:8000",
        "secure": false,
        "changeOrigin": true,
        "logLevel": "debug",
        "timeout": 120000,
        "proxyTimeout": 120000,
        "headers": {
            "Connection": "keep-alive"
        }
    },
    "/media/": {
        "target": "http://backend:8000",
        "secure": false,
        "changeOrigin": true
    }
}
